{
  "name": "aviation-stand-finder",
  "version": "1.0.0",
  "description": "Real-world parking stand/gate identification for arriving flights",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:seed": "tsx src/scripts/seed.ts",
    "import:airports": "tsx src/scripts/import-airports.ts",
    "import:stands": "tsx src/scripts/import-stands.ts"
  },
  "keywords": [
    "aviation",
    "flight",
    "stand",
    "gate",
    "parking",
    "msfs",
    "simulator",
    "ads-b",
    "opensky"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^7.0.1",
    "@prisma/client": "^5.18.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "node-cache": "^5.1.2",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "redis": "^4.7.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "ts-jest": "^29.2.4",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
