# Server Configuration
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# Database
DATABASE_URL="file:./dev.db"

# Redis (optional)
REDIS_URL=redis://localhost:6379
USE_REDIS=false

# API Keys (free tier)
ADSBEXCHANGE_API_KEY=
AVIATIONSTACK_API_KEY=

# OpenSky Network (no key required for basic tier)
OPENSKY_USERNAME=
OPENSKY_PASSWORD=

# Cache Configuration
CACHE_TTL_SECONDS=3600
FLIGHT_CACHE_TTL_SECONDS=86400

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_TIME_WINDOW=60000

# Logging
LOG_LEVEL=info
LOG_PRETTY=true

# CORS
CORS_ORIGIN=*

# Crowdsourcing
REQUIRE_MODERATION=true
MIN_CONFIDENCE_SCORE=0.5
